<h1>Listing matches</h1>

<table>
  <tr>
    <th>Team</th>
    <th>Team2</th>
    <th>Score1</th>
    <th>Score2</th>
    <th>Points1</th>
    <th>Points2</th>
  </tr>

<% @matches.each do |match| %>
  <tr>
    <td><%=h match.team_id %></td>
    <td><%=h match.team2_id %></td>
    <td><%=h match.score1 %></td>
    <td><%=h match.score2 %></td>
    <td><%=h match.points1 %></td>
    <td><%=h match.points2 %></td>
    <td><%= link_to 'Show', match %></td>
    <td><%= link_to 'Edit', edit_match_path(match) %></td>
    <td><%= link_to 'Destroy', match, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New match', new_match_path %>
<%= link_to 'Free match', new_calendar_path %>
