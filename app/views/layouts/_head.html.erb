    <script type="text/javascript">   //<![CDATA[

    function hideFlash()
    { 
      var flashDiv = $("flash");
      if (flashDiv) {
        flashDiv.childElements().each(function(element) {
          new Effect.Highlight(element,{
                        startcolor: "#ffff00",
                        endcolor: "#ffffff",
                        duration: 3
                      })

          new Effect.SlideUp("flash", { "delay": 6, "duration": 2 });
        });
      }
    }
    function focusedAction(el, val)
    {
    if(el.value == val)
      el.value = ''; 
    new Effect.Highlight(el,{});
    }

    function checkEmptyAndFill(el, val)
    {
      if(el.value.blank())
        el.value = val;
    }
    function menu_hover(style1, style2)
    {
      $('header_over').setStyle({background: style1});
      $('header_over1').setStyle({background: style2});
    }
    function menu_down()
    {
      $('header_over').setStyle({background: 'none'});
      $('header_over1').setStyle({background: 'none'});
    }
    //]]>
		</script>
