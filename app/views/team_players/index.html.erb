<h1>Listing team_players</h1>

<table>
  <tr>
    <th>Team</th>
    <th>User</th>
  </tr>

<% @team_players.each do |team_player| %>
  <tr>
    <td><%=h team_player.team_id %></td>
    <td><%=h team_player.user_id %></td>
    <td><%= link_to 'Show', team_player %></td>
    <td><%= link_to 'Edit', edit_team_player_path(team_player) %></td>
    <td><%= link_to 'Destroy', team_player, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New team_player', new_team_player_path %>